@model MonthlyReport.Models.Sales
@{
    ViewBag.Title = "Sales - Square & Lifestyle";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int i = 0;
    int j = 0;
    int role = Convert.ToInt32(Session["roleid"].ToString());
}


<br />
@if (role == 1 || role == 4)
{
<div class="row">
    <div class="col-sm-10"></div>
    <div class="col-sm-2">
        @Html.ActionLink("Edit", "SquareEdit", "Sales", null, new { @class = "btn btn-primary" })
    </div>
</div>
}
