
@{
    ViewBag.Title = "Completed Renewals";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int i = 0;
}

<script type="text/javascript">
    $(document).ready(function () {
        var mod = @Html.Raw(Json.Encode(Model));
      // alert(JSON.stringify(mod));
        for (var j = 1; j < 10; j++) {
            var dropdown = document.getElementById("Sector" + j);
            if (dropdown != null) {
              dropdown.value = mod[j - 1].Sector;
            }
        }

    });

    function resetform() {
        $("input:text").val("");

        $("textarea").val("");

        for (var j = 1; j < 10; j++) {
            var dropdown = document.getElementById("Sector" + j);
            if (dropdown != null) {
                dropdown.value = "Sector";
            }
        }
    };

</script>
<div class="row">
    <div class="col-sm-10">

    </div>
    <div class="col-sm-1">
        <button class="btn btn-primary" onclick="resetform();">Reset</button>
    </div>
    <div class="col-sm-1">
        @Html.ActionLink("Cancel", "Index", "Leasing", null, new { @class = "btn btn-primary" })
    </div>
</div>


@using (Html.BeginForm("CompletedRenewalsEdit", "Leasing", FormMethod.Post))
{

    <div class="row">
        <div class=" col-sm-3">
            <h5><strong>COMPLETED RENEWALS</strong></h5>
        </div>
        <div class=" col-sm-2">
            @*<select id="month" name="month" class=" form-control">
                    <option value="">--Select--</option>
                    <option value="January">January</option>
                    <option value="February">February</option>
                    <option value="March">March</option>
                    <option value="April">April</option>
                    <option value="May">May</option>
                    <option value="June">June</option>
                    <option value="July">July</option>
                    <option value="August">August</option>
                    <option value="September">September</option>
                    <option value="October">October</option>
                    <option value="November">November</option>
                    <option value="Decemeber">Decemeber</option>

                </select>*@
        </div>

