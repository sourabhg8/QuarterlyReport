                    </th>
                    <th style="width : 8%; border-right : 1px solid black">
                        <input type="text" value="@obj.Gla" name=@("Gla" + i) id=@("Gla" + i) class="form-control" style=" width : 100%;font-size : 14px; border : none" />
                    </th>
                    <th style="width : 8%; border-right : 1px solid black">

                        <input type="text" value="@obj.Term" name=@("Term" + i) id=@("Term" + i) class="form-control" style=" width : 100%;font-size : 14px; border : none" />

                    </th>
                    <th style="width : 8%; border-right : 1px solid black">
                        <div style="float : left; width : 8%">$</div><div style="float: left; width : 90%;">
                            <input type="text" value="@obj.NetBudget" name=@("NetBudget" + i) id=@("NetBudget" + i) class="form-control textboxrightalign" style=" width : 100%;font-size : 14px; border : none" onblur="focuslost(this)" />
                        </div>
                    </th>

                    <th style="width : 8%; border-right : 1px solid black">
                        <div style="float : left; width : 8%">$</div><div style="float: left; width : 90%;">
                            <input type="text" value="@obj.NetRentActual" name=@("NetRentActual" + i) id=@("NetRentActual" + i) class="form-control textboxrightalign" style=" width : 100%;font-size : 14px; border : none" onblur="focuslost(this)" />
                        </div>
                    </th>

                    <th style="width : 8%; border-right : 1px solid black">
                        <div style="float : left; width : 8%">$</div><div style="float: left; width : 90%;">
                            <input type="text" value="@obj.Ner" name=@("Ner" + i) id=@("Ner" + i) class="form-control textboxrightalign" style=" width : 100%;font-size : 14px; border : none" onblur="focuslost(this)" />
                        </div>
                    </th>
                    <th style="width : 8%; border-right : 1px solid black">
                        <div style="float : left; width : 8%">$</div><div style="float: left; width : 90%;">
                            <input type="text" value="@obj.Llw" name=@("Llw" + i) id=@("llw" + i) class="form-control textboxrightalign" style=" width : 100%;font-size : 14px; border : none" onblur="focuslost(this)" />
                        </div>
                    </th>
                    <th style="width : 8%; border-right : 1px solid black">
                        <div style="float : left; width : 8%">$</div><div style="float: left; width : 90%;">
                            <input type="text" value="@obj.TA" name=@("TA" + i) id=@("TA" + i) class="form-control textboxrightalign" style=" width : 100%;font-size : 14px; border : none" onblur="focuslost(this)" />
                        </div>
                    </th>
                    <th style="width : 8%; ">
                        <div style="float : left; width : 8%">$</div><div style="float: left; width : 90%;">
                            <input type="text" value="@obj.Commision" name=@("Commision" + i) id=@("Commision" + i) class="form-control textboxrightalign" style=" width : 100%;font-size : 14px; border : none" onblur="focuslost(this)" />
                        </div>
                    </th>
                </tr>
            }

        </table>
    </div>
    <div class="row">
        <div class="col-sm-11"></div><div class="col-sm-1"> <input type="submit" value="Save" class="btn btn-primary" /></div>
    </div>
}



