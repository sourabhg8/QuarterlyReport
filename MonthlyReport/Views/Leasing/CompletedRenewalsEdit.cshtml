                <th style="width : 8%; vertical-align : middle"><h5 class="closetotable">Commision</h5></th>
            </tr>
            @foreach (var obj in Model)
            {
                i++;
                <tr style="border-bottom : 1px solid Black;" class="MetricName" !important>
                    <th style="width : 20%; border-right : 1px solid black">
                        <input type="text" value="@obj.Tenant" name=@("Tenant" + i) id=@("Tenant" + i) class="form-control" style=" width : 100%;font-size : 14px; border : none" />
                    </th>
                    <th style="width : 8%; border-right : 1px solid black">
                        <select id=@("Sector" + i) name=@("Sector" + i) class=" form-control" style="font-size : 14px">
